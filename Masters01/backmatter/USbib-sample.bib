	issn = {15730409},
	journal = {Journal of Intelligent and Robotic Systems: Theory and Applications},
	keywords = {Complete coverage,DARP,MFC,MSTC,Minimum coverage paths,Multi-robots,Terrain sub-division,mCPP},
