	issn = {10122443},
	journal = {Annals of Mathematics and Artificial Intelligence},
	keywords = {Cell decompositions,Coverage,Mobile robots},
	mendeley-groups = {Motion Planning/Coverage Path Planning/Surveys,Motion Planning/Coverage Path Planning,THESIS/Introduction},
	title = {{Coverage for robotics - A survey of recent results}},
	year = {2001}
}

